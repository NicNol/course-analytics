{
  "name": "course-analytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run scrape",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prescrape": "npm run transpile",
    "scrape": "node ./util/scheduler/updateDB.js",
    "prescrape-dev": "npx tsc ./util/scheduler/updateDB.ts",
    "scrape-dev": "node -r dotenv/config ./util/scheduler/updateDB.js dotenv_config_path=./.env.local",
    "pretranspile": "npm install -g typescript",
    "transpile": "npx tsc ./util/scheduler/updateDB.ts",
    "cypress-dev": "cypress open",
    "cypress": "cypress run"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^2.4.2",
    "@vercel/analytics": "^0.1.5",
    "framer-motion": "^6.2.8",
    "google-spreadsheet": "^3.2.0",
    "inngest": "^0.8.5",
    "mongoose": "^6.4.6",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.198",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.10",
    "@types/react": "17.0.33",
    "cypress": "^10.0.3",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.1",
    "prettier": "2.4.1",
    "typescript": "^4.9.3"
  },
  "cacheDirectories": [
    ".next/cache"
  ]
